{
  "expo": {
    "name": "Dengo",
    "slug": "dengo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF7043"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielvaz.dengo",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF7043"
      },
      "package": "com.gabrielvaz.dengo",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FF7043",
          "image": "./assets/splash-icon.png",
          "imageUrl": "./assets/splash-icon.png"
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "legal": {
        "privacyPolicyUrl": "https://raw.githubusercontent.com/gabrielvaz/dengo-app-2026/main/app-release/05-legal/privacy-policy-pt-BR.md",
        "termsOfUseUrl": "https://raw.githubusercontent.com/gabrielvaz/dengo-app-2026/main/app-release/05-legal/terms-of-use-pt-BR.md",
        "supportEmail": "contato@dengo.app"
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
